<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>QIWI charity widgets</title>
    <link rel="stylesheet" type="text/css" href="css/style.css">
</head>
<body>

    <% widgets.forEach(function(widget, index) { %>

        <h1>widget <%= ++index %> </h1>

        <iframe width="300" height="300" src="<%= widget %>?public_key=<%= public_key %>" allowtransparency="true" scrolling="no" frameborder="0"></iframe>

    <% }); %>

</body>
</html>